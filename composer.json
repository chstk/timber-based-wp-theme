{
  "name": "blank",
  "type": "Wordpress-theme",
  "description": "Wordpress Theme Boilerplate",
  "license": "GNU GPL",
  "authors": [
    {
      "name": "Andrew Dudin",
      "email": "iluxor@yandex.ru"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "twig/twig": "^2.4.3",
    "timber/timber": "^1.3.3",
    "phpmailer/phpmailer": "^5.2",
    "htmlburger/carbon-fields": "^1.6"
  },
  "autoload": {
      "psr-4": {
         "App\\": "app/"
      }
  },
  "config": {
        "vendor-dir": "core/vendor",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "installer-paths": {
            "core/vendor/{$name}": ["htmlburger/carbon-fields"]
        }
    }
}
