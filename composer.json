{
  "name": "blank",
  "type": "wp-theme",
  "description": "Wordpress Theme Boilerplate",
  "license": "GNU GPL",
  "authors": [
    {
      "name": "Andrew Dudin",
      "email": "iluxor1991@gmail.com"
    }
  ],
  "repositories":[
      {
          "type":"composer",
          "url":"https://wpackagist.org"
      }
  ],
  "require": {
    "php": "^7.0",
    "htmlburger/carbon-fields": "^3.1.1",
    "phpmailer/phpmailer": "*",
    "timber/timber": "^1.9.4",
    "twig/twig": "^2.8.1",
    "wpackagist-plugin/all-in-one-wp-migration": "*",
    "wpackagist-plugin/cyr3lat": "*",
    "wpackagist-plugin/polylang": "*",
    "wpackagist-plugin/tinymce-advanced": "*",
    "wpackagist-plugin/wordpress-seo": "*"
  },
  "autoload": {
      "psr-4": {
         "App\\": "app/"
      }
  },
  "config": {
        "vendor-dir": "core/vendor",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "installer-paths": {
            "core/vendor/{$name}": ["htmlburger/carbon-fields"],
            "../../plugins/{$name}": [
              "wpackagist-plugin/tinymce-advanced",
              "wpackagist-plugin/cyr3lat",
              "wpackagist-plugin/all-in-one-wp-migration",
              "wpackagist-plugin/polylang",
              "wpackagist-plugin/wordpress-seo"
            ]
        }
    }
}
